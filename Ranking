<!-- Responsive Select JS
================================================================ -->
<script type="text/javascript">
	jQuery(function(){
		jQuery( '.responsiveMenuSelect' ).change(function() {
			var loc = $(this).find( 'option:selected' ).val();
			if( loc != '' && loc != '#' ) window.location = loc;
		});
		//$( '.responsiveMenuSelect' ).val('');
	});
</script>
